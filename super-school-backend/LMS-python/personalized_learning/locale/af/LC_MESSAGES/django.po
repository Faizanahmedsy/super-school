#: personalized_learning/views.py:46 personalized_learning/views.py:397
msgid "school_id is required."
msgstr "school_id is vereis."

#: personalized_learning/views.py:67
msgid ""
"You have already generated 40 quizzes for this Class and Subject. You cannot "
"generate more."
msgstr ""
"Jy het reeds 40 vasvrae vir hierdie klas en vak gegenereer. Jy kan nie meer "
"genereer nie."

#: personalized_learning/views.py:72 personalized_learning/views.py:408
msgid "Both quiz_start_date_time and quiz_end_date_time are required."
msgstr "Beide quiz_start_date_time en quiz_end_date_time is vereis."

#: personalized_learning/views.py:75 personalized_learning/views.py:413
#: personalized_learning/views.py:591
msgid "quiz_start_date_time must be earlier than quiz_end_date_time."
msgstr "quiz_start_date_time moet vroeër wees as quiz_end_date_time."

#: personalized_learning/views.py:78 personalized_learning/views.py:296
msgid "Number of questions must be less than or equal to 25."
msgstr "Die aantal vrae moet minder as of gelyk aan 25 wees."

#: personalized_learning/views.py:81
msgid "Grade and subject are required."
msgstr "Graad en vak is vereis."

#: personalized_learning/views.py:83 personalized_learning/views.py:428
msgid "grade_class must be a non-empty list."
msgstr "grade_class moet 'n nie-leë lys wees."

#: personalized_learning/views.py:97
msgid "No exam result found,Try generating quiz after exam"
msgstr ""
"Geen eksamenuitslae gevind nie. Probeer om die vasvra na die eksamen te "
"genereer."

#: personalized_learning/views.py:118 personalized_learning/views.py:328
msgid "Failed to generate quiz with AI. Please try again later."
msgstr "Kon nie die vasvra met AI genereer nie. Probeer asseblief later weer."

#: personalized_learning/views.py:135 personalized_learning/views.py:452
#, python-format
msgid "GradeClass with ID %(grade_class_id)s does not exist."
msgstr "GradeClass met ID %(grade_class_id)s bestaan nie."

#: personalized_learning/views.py:168 personalized_learning/views.py:483
#, python-format
msgid "No students found for grade class %(grade_class_id)s."
msgstr "Geen studente gevind vir graadklas %(grade_class_id)s nie."

#: personalized_learning/views.py:240
#, python-format
msgid "Quizzes created successfully for %(num_learners)s learners."
msgstr "Vasvrae suksesvol geskep vir %(num_learners)s leerders."

#: personalized_learning/views.py:256
msgid "You do not have permission to perform this action."
msgstr "Jy het nie toestemming om hierdie aksie uit te voer nie."

#: personalized_learning/views.py:261
msgid "Term is required."
msgstr "Termyn is vereis."

#: personalized_learning/views.py:275
msgid ""
"You have already generated 25 quizzes for this TERM. You cannot generate "
"more."
msgstr ""
"Jy het reeds 25 vasvrae vir hierdie termyn gegenereer. Jy kan nie meer "
"genereer nie."

#: personalized_learning/views.py:302
msgid "grade_id required for generating quiz."
msgstr "grade_id is vereis om 'n vasvra te genereer."

#: personalized_learning/views.py:307
msgid "subject_id is required."
msgstr "subject_id is vereis."

#: personalized_learning/views.py:312
msgid "weaknesses is required."
msgstr "swakhede is vereis."

#: personalized_learning/views.py:384
msgid "Quiz generated successfully"
msgstr "Vasvra suksesvol gegenereer."

#: personalized_learning/views.py:557
#, python-format
msgid "Quizzes created successfully for %(count)d quizzes."
msgstr "Vasvrae suksesvol geskep vir %(count)d vasvrae."

#: personalized_learning/views.py:573
#, python-format
msgid "MainQuiz with id %(pk)s does not exist."
msgstr "MainQuiz met id %(pk)s bestaan nie."

#: personalized_learning/views.py:618
msgid "Each QuizQandA object must include its 'id' for update."
msgstr "Elke QuizQandA-objek moet sy 'id' insluit vir opdatering."

#: personalized_learning/views.py:626
#, python-format
msgid ""
"QuizQandA with id %(qanda_id)s does not exist for the provided MainQuiz."
msgstr "QuizQandA met id %(qanda_id)s bestaan nie vir die gegewe MainQuiz nie."

